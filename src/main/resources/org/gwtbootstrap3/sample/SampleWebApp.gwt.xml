<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='SampleWebApp'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name='org.gwtbootstrap3.GwtBootstrap3' />

	<!-- Specify the app entry point class. -->
	<entry-point class='org.gwtbootstrap3.sample.client.SampleWebApp' />

	<!--For SDM -->
	<add-linker name="xsiframe" />
	<set-configuration-property name="devModeRedirectEnabled"
		value="true" />
	<set-configuration-property name="gwt.superdevmode"
		value="on" />

	<!-- for offline mode -->
	<inherits name="org.gwtbootstrap3.extras.cachemanifest.Offline" />
	<extend-configuration-property name="cachemanifest_static_files" value="SampleWebApp.html" />
	<add-linker name="offline" />

	<!-- Specify the paths for translatable code -->
	<source path='client' />

</module>
